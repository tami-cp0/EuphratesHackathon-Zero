<% layout('layout') %> <% var title = "Stock Market Research" %>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Stock Market Research</h3>
      </div>
      <div class="panel-body">
        <form action="/research" method="get" role="search">
          <div class="form-group">
            <input type="text" class="form-control" placeholder="Stock Symbol" name="symbol" />
            <p class="help-block">Enter Stock Symbol.</p>
          </div>
          <button type="submit" class="btn btn-primary">Submit</button>
        </form>
      </div>
    </div>
  </div>
</div>
